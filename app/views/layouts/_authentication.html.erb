<% if current_user %>
    Signed in as <%= current_user.email %>
    <%= link_to 'My Cars', my_cars_path %>
    <%= link_to 'Logout', logout_path,
                method: :delete,
                :data => { confirm: 'Are you sure?' } %>
<% else %>
    <%= link_to 'Signup', new_user_path %> |
    <%= link_to 'Login', login_path %>
<% end %>
